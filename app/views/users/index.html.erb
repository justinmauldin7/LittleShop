<section class="jumbotron">
  <i class="fas fa-store"></i>
  <h1>All Merchants</h1>
</section>

<div class="top-three-merch-quantity">
  <% @top_three_quantity.each do |merch| %>
    <%= merch.name %>
  <% end  %>
</div>

<div class="top-three-merch-revenue">
  <% @top_three_revenue.each do |merch| %>
    <%= merch.name %>
  <% end  %>
</div>

<div class="top-three-merch-fulfillment">
  <% @top_three_by_avg_fulfill_time.each do |merch| %>
    <%= merch.name %>
  <% end  %>
</div>

<div class="bottom-three-merch-fulfillment">
  <% @bottom_three_by_avg_fulfill_time.each do |merch| %>
    <%= merch.name %>
  <% end  %>
</div>

<div class="top-three-states">
  <% @top_three_states.each do |user| %>
    <%= user.state %>
  <% end  %>
</div>

<div class="top-three-cities">
  <% @top_three_cities.each do |user| %>
    <%= "#{user.city}, #{user.state}" %>
  <% end  %>
</div>

<div class="top-three-largest-orders">
  <% @top_three_largest_orders.each do |order| %>
    <%= "#{order.id}, #{order.quantity}" %>
  <% end  %>
</div>

<% @mechants.each do |merchant| %>
  <%= merchant.name %>
  <%= merchant.city %>
  <%= merchant.state %>
  <%= merchant.created_at %>
<% end %>
